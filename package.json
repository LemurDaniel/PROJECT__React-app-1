{
  "homepage": "/",
  "name": "doodles-react-spa",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.2.6",
    "chokidar": "^3.5.2",
    "matter-js": "^0.18.0",
    "npm": "^8.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "roughjs": "^4.4.1",
    "tailwindcss": "^3.0.15",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:CSS && react-scripts build",
    "build:CSS": "tailwindcss build ./src/css/Globals.css -o ./src/css/Tailwind.css",
    "build:Docker": "docker build -t daniellandau1998/node-server -f ./docker/df-webserver . && docker push daniellandau1998/node-server",
    "server:deploy": "docker pull daniellandau1998/node-server && docker-compose -f ./docker/deploy/dc-web-db.yaml up -d",
    "server": "docker-compose -f ./docker/dc-web-db.yaml up"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

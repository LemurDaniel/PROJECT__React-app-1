{
  "name": "react_app_1",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "autoprefixer": "^10.2.6",
    "move-file-cli": "^2.0.0",
    "npx": "^10.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "tailwindcss": "^2.1.4",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:CSS": "tailwindcss build ./src/css/Globals.css -o ./src/css/Tailwind.css",
    "build:Docker": "docker build -t daniellandau1998/node-webserver -f ./docker/df-webserver .",
    "build:React": "react-scripts build && npx move-file ./build ./server/build",
    "build:local": "npm run build:CSS && rmdir /s/q \"./server/build\" && npm run build:React && npm run build:Docker",
    "build:deploy": "npm run build:CSS && npm run build:React",
    "server": "docker-compose -f ./docker/dc-web-db.yaml up",
    "build:test": "npm run build && npm run server"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
